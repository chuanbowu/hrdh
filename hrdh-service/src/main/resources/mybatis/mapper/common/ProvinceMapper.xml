<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.flym.hrdh.mapper.single.common.ProvinceMapper" >
  <resultMap id="BaseResultMap" type="com.flym.hrdh.pojo.common.Province" >
    <result column="name" property="name" jdbcType="VARCHAR" />
    <result column="cid" property="cid" jdbcType="VARCHAR" />
  </resultMap>
  <insert id="insert" parameterType="com.flym.hrdh.pojo.common.Province" >
    insert into c_province (name, cid)
    values (#{name,jdbcType=VARCHAR}, #{cid,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" parameterType="com.flym.hrdh.pojo.common.Province" >
    insert into c_province
    <trim prefix="(" suffix=")" suffixOverrides="," >
      <if test="name != null" >
        name,
      </if>
      <if test="cid != null" >
        cid,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides="," >
      <if test="name != null" >
        #{name,jdbcType=VARCHAR},
      </if>
      <if test="cid != null" >
        #{cid,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>

  <select id="findProvinceList" resultMap="BaseResultMap">
    select
    name, cid
    from c_province
  </select>
</mapper>